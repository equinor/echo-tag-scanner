{
  "bundler": "webpack",
  "webpackConfigPath": "./webpack/webpack.config.js",
  "manifest": {
    "name": "Echo Tag Scanner",
    "key": "echo-tag-scanner",
    "shortName": "ets",
    "path": "/newcamera",
    "description": "An Echo module where the users can do searches for tags by taking pictures of tag signs.",
    "private": false,
    "fileUri": "https://echocamera.equinor.com/index.js",
    "devFileUri": "https://echocamera-dev.equinor.com/index.js",
    "qaFileUri": "https://echocamera.equinor.com/index.js",
    "testFileUri": "https://echocamera-dev.equinor.com/index.js"
  },
  "server": {
    "contentBase": "/build",
    "port": 3000,
    "open": false,
    "host": "localhost",
    "logging": "none"
  },
  "devMiddleware": {
    "stats": "errors-warnings"
  }
}