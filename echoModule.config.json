{
  "bundler": "webpack",
  "webpackConfigPath": "./webpack.config.js",
  "manifest": {
    "name": "Echo Camera Web",
    "key": "echo-camera-web",
    "shortName": "ecw",
    "path": "/camera",
    "description": "A web based tag scanner",
    "private": false,
    "fileUri": "https://echocamera.equinor.com/index.js",
    "devFileUri": "https://echocamera-dev.equinor.com/index.js",
    "qaFileUri": "https://echocamera-dev.equinor.com/index.js",
    "testFileUri": "https://echocamera-dev.equinor.com/index.js"
  },
  "server": {
    "contentBase": "/build",
    "port": 3000,
    "open": false,
    "host": "localhost",
    "logging": "none"
  },
  "devMiddleware": {
    "stats": "errors-warnings"
  }
}