{
  "include": ["src"],
  "compilerOptions": {
    "target": "es2017",
    "lib": ["esnext", "dom", "dom.iterable"],
    "allowJs": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "esnext",
    "removeComments": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "types": [
      "@equinor/echo-scripts",
      "@types/webpack-env",
      "@types/node",
      "@types/w3c-image-capture",
      "./node_modules/user-agent-data-types"
    ],
    "baseUrl": ".",
    "paths": {
      "@ui": ["src/UI/"],
      "@cameraLogic": ["src/cameraLogic/"],
      "@const": ["src/const/"],
      "@hooks": ["src/hooks/"],
      "@services": ["src/services/"],
      "@types": ["src/types/"],
      "@utils": ["src/utils/"]
    }
  }
}
